{
  "name": "forms-and-state",
  "description": "Demo application for complete reactive forms solution",
  "author": "EastBanc Technologies",
  "version": "1.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eastbanctechru/forms-and-state/issues"
  },
  "homepage": "https://github.com/eastbanctechru/forms-and-state#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eastbanctechru/forms-and-state.git"
  },
  "scripts": {
    "prettier": "prettier --write \"./src/**/!(*.d.ts|*.png|*.svg|*.ico|*.html)\"",
    "generate-types": "sw2dts --namespace Backend -o ./src/app/backend.d.ts ./swagger.json",
    "ng": "ng",
    "build": "npm run generate-types && ng lint && ng build --named-chunks false",
    "build:prod": "ng lint && ng build --configuration production",
    "bundle-report": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json",
    "watch": "npm run generate-types && ng lint && ng serve",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "6.1.4",
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "@ng-select/ng-select": "^2.5.0",
    "angular-i18next": "4.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "2.5.7",
    "i18next": "11.6.0",
    "i18next-xhr-backend": "1.5.1",
    "moment": "^2.22.2",
    "ngx-cookie": "^4.0.2",
    "rxjs": "6.2.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.4",
    "@angular/language-service": "6.1.4",
    "@types/i18next": "8.4.4",
    "@types/i18next-xhr-backend": "1.4.1",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "10.9.2",
    "codelyzer": "4.4.4",
    "prettier": "1.14.2",
    "sw2dts": "2.6.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "webpack-bundle-analyzer": "2.13.1"
  }
}
